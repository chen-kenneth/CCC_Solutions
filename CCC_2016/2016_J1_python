g1 = input()
g2 = input()
g3 = input()
g4 = input()
g5 = input()
g6 = input()
counter = 0

if g1 == "W":
  counter+=1
if g2 == "W":
  counter+=1
if g3 == "W":
  counter+=1
if g4 == "W":
  counter+=1
if g5 == "W":
  counter+=1
if g6 == "W":
  counter+=1

if counter == 5 or counter == 6:
  print(1)
elif counter == 3 or counter == 4:
  print(2)
elif counter == 1 or counter == 2:
  print(3)
else:
  print(-1)